        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "5.6.*",
        "ext-json": "*"
    },
    "autoload": {
        "classmap": [
            "lib/",
            "../../../wp-includes",
            "../woocommerce/includes"
        ],
        "exclude-from-classmap": [
            "../woocommerce/includes/api/legacy",
            "../woocommerce/includes/api/v1",
            "../woocommerce/includes/api/v2"
        ]
    }
}
